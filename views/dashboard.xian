    <style>
      /* Premium Animations */
      @keyframes float {
        0%, 100% { transform: translateY(0px) rotate(0deg); }
        50% { transform: translateY(-10px) rotate(1deg); }
      }
      
      @keyframes glow {
        0%, 100% { box-shadow: 0 0 20px rgba(99, 102, 241, 0.3), 0 0 40px rgba(99, 102, 241, 0.1); }
        50% { box-shadow: 0 0 30px rgba(99, 102, 241, 0.5), 0 0 60px rgba(99, 102, 241, 0.2); }
      }
      
      @keyframes shimmer {
        0% { background-position: -200% 0; }
        100% { background-position: 200% 0; }
      }
      
      @keyframes particle-float {
        0%, 100% { transform: translateY(0px) translateX(0px); opacity: 0.3; }
        25% { transform: translateY(-20px) translateX(10px); opacity: 0.7; }
        50% { transform: translateY(-10px) translateX(-5px); opacity: 0.5; }
        75% { transform: translateY(-30px) translateX(15px); opacity: 0.8; }
      }
      
      /* Premium Components */
      .premium-badge {
        @apply px-4 py-2 bg-white/20 rounded-full text-sm backdrop-blur-sm border border-white/30 flex items-center gap-2 hover:bg-white/30 transition-all duration-300 cursor-default;
      }
      
      .premium-button {
        @apply px-8 py-4 rounded-2xl font-semibold transition-all duration-300 transform hover:scale-105 shadow-lg hover:shadow-2xl backdrop-blur-sm border border-white/20 flex items-center justify-center;
        animation: glow 3s ease-in-out infinite;
      }
      
      .premium-card {
        @apply bg-white/95 backdrop-blur-xl rounded-3xl shadow-2xl border border-white/20 hover:shadow-3xl transition-all duration-500 transform hover:-translate-y-2;
        background: linear-gradient(135deg, rgba(255,255,255,0.95) 0%, rgba(255,255,255,0.9) 100%);
      }
      
      .stats-card {
        @apply rounded-3xl shadow-2xl p-8 text-white relative overflow-hidden transition-all duration-500 transform hover:-translate-y-3 hover:shadow-3xl;
        background: linear-gradient(135deg, var(--gradient-from) 0%, var(--gradient-to) 100%);
        border: 1px solid rgba(255,255,255,0.2);
      }
      
      .stats-card::before {
        content: '';
        position: absolute;
        top: 0;
        left: -100%;
        width: 100%;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(255,255,255,0.2), transparent);
        animation: shimmer 3s infinite;
      }
      
      .stats-card::after {
        content: '';
        position: absolute;
        top: -50%;
        right: -50%;
        width: 200px;
        height: 200px;
        background: radial-gradient(circle, rgba(255,255,255,0.1) 0%, transparent 70%);
        border-radius: 50%;
        animation: float 6s ease-in-out infinite;
      }
      
      .action-card {
        @apply group relative bg-gradient-to-br rounded-2xl p-6 transition-all duration-500 transform hover:scale-105 hover:shadow-2xl border-2 border-transparent;
        background: linear-gradient(135deg, var(--bg-from) 0%, var(--bg-to) 100%);
      }
      
      .action-card::before {
        content: '';
        position: absolute;
        inset: 0;
        padding: 2px;
        background: linear-gradient(135deg, var(--border-from), var(--border-to));
        border-radius: inherit;
        mask: linear-gradient(#fff 0 0) content-box, linear-gradient(#fff 0 0);
        mask-composite: exclude;
        opacity: 0;
        transition: opacity 0.3s;
      }
      
      .action-card:hover::before {
        opacity: 1;
      }
      
      .floating-particles {
        position: absolute;
        width: 100%;
        height: 100%;
        overflow: hidden;
      }
      
      .floating-particles::before,
      .floating-particles::after {
        content: '';
        position: absolute;
        width: 4px;
        height: 4px;
        background: rgba(255,255,255,0.3);
        border-radius: 50%;
        animation: particle-float 8s infinite linear;
      }
      
      .floating-particles::before {
        top: 20%;
        left: 20%;
        animation-delay: -2s;
      }
      
      .floating-particles::after {
        top: 60%;
        right: 30%;
        animation-delay: -4s;
      }
      
      .glass-morphism {
        background: rgba(255, 255, 255, 0.1);
        backdrop-filter: blur(20px);
        border: 1px solid rgba(255, 255, 255, 0.2);
      }
      
      /* Responsive Enhancements */
      @media (max-width: 768px) {
        .premium-button {
          @apply px-6 py-3 text-sm;
        }
        
        .stats-card {
          @apply p-6;
        }
      }
    </style>

